{
    "version": 2,
    "routes": [
        {
            "src": "/404.html",
            "status": 404,
            "dest": "/404.html"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/404.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)\\.(?:jpg|jpeg|gif|png|ico|webp)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=2592000"
                }
            ]
        },
        {
            "source": "/(.*)\\.(?:css|js)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=604800"
                }
            ]
        },
        {
            "source": "/(.*)\\.(?:html|xml)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=43200"
                }
            ]
        }
    ]
}